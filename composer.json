{
    "name": "avto-dev/json-rpc-laravel",
    "description": "Json Rpc package",
    "keywords": [
        "json",
        "laravel",
        "rpc"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Avto Develop",
            "homepage": "https://github.com/avto-dev"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/contracts": ">=5.5.0 <5.9.0 || ~6.0",
        "illuminate/support": ">=5.5.0 <5.9.0 || ~6.0",
        "illuminate/http": ">=5.5.0 <5.9.0 || ~6.0",
        "illuminate/routing": ">=5.5.0 <5.9.0 || ~6.0",
        "tarampampam/wrappers-php": "^1.4"
    },
    "require-dev": {
        "laravel/laravel": ">=5.5.0 <5.9.0 || ~6.0",
        "phpunit/phpunit": "^6.4 || ~7.5",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^0.11.3",
        "fzaninotto/faker": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "AvtoDev\\JsonRpc\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AvtoDev\\JsonRpc\\Tests\\": "tests"
        }
    },
    "scripts": {
        "phpunit": "@php ./vendor/bin/phpunit --no-coverage --colors=always",
        "phpunit-cover": "@php ./vendor/bin/phpunit --coverage-html='./coverage/html'",
        "phpstan": "@php ./vendor/bin/phpstan analyze -c ./phpstan.neon.dist --no-progress --ansi",
        "test": [
            "@phpstan",
            "@phpunit"
        ],
        "test-cover": [
            "@phpstan",
            "@phpunit-cover"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "AvtoDev\\JsonRpc\\ServiceProvider"
            ],
            "aliases": {
                "RpcRouter": "AvtoDev\\JsonRpc\\RpcRouter"
            }
        }
    },
    "support": {
        "issues": "https://github.com/avto-dev/json-rpc-laravel/issues",
        "source": "https://github.com/avto-dev/json-rpc-laravel"
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
